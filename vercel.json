{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.3.1"
        }
    },
    "routes": [
        {
            "src": "/(css|js|img|font)/(.*)",
            "dest": "webroot/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ]
}
